{
	"name": "@rbxts/pretty-vide-utils",
	"version": "1.0.0",
	"description": "Useful utilities for @rbxts/vide",
	"main": "out/init.lua",
	"scripts": {
		"watch": "rbxtsc -w --type=game -p test -i test/include",
		"dev": "rbxtsc -w --type game --rojo test.project.json",
		"serve": "rojo serve test/default.project.json"
	},
	"keywords": [
		"vide",
		"rbxts",
		"hooks",
		"pretty-react-hooks"
	],
	"author": "PepeElToro",
	"license": "MIT",
	"types": "out/index.d.ts",
	"files": [
		"out",
		"!**/*.tsbuildinfo",
		"!**/*.spec.lua"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@rbxts/compiler-types": "^2.3.0-types.2",
		"@rbxts/testez": "^0.4.2-ts.0",
		"@rbxts/types": "^1.0.805",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-roblox-ts": "^0.0.35",
		"prettier": "^2.8.8",
		"roblox-ts": "^2.3.0-dev-ff69514",
		"typescript": "=5.5.3"
	},
	"dependencies": {
		"@rbxts/ripple": "^0.9.0",
		"@rbxts/set-timeout": "^1.1.2",
		"@rbxts/vide": "^0.4.5",
		"@rbxts/services": "^1.5.1"
	}
}
